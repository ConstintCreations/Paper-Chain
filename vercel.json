{
    "version": 2,
    "builds": [
        { "src": "api/**/*.js", "use": "@vercel/node" },
        { "src": "public/**", "use": "@vercel/static" }
    ],
    "routes": [
        { "src": "/api/coins", "dest": "/api/coins.js" },
        { "src": "/api/coin/history", "dest": "/api/coin/history.js" },
        { "src": "/(.*)", "dest": "/public/$1" }
    ]
}